{
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "profiles": {
    "prod": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "swagger",
      "environmentVariables": {
        "ASPNETCORE_URLS": "https://+:5443;http://+:5000",
        "ASPNETCORE_ENVIRONMENT": "prod"
      },
      "applicationUrl": "http://localhost:5000"
    },
    "qa": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "swagger",
      "environmentVariables": {
        "ASPNETCORE_URLS": "https://+:5443;http://+:5000",
        "ASPNETCORE_ENVIRONMENT": "qa"
      },
      "applicationUrl": "http://localhost:5000"
    },
    "test": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "swagger",
      "environmentVariables": {
        "ASPNETCORE_URLS": "https://+:5443;http://+:5000",
        "ASPNETCORE_ENVIRONMENT": "test"
      },
      "applicationUrl": "http://localhost:5000"
    },
    "Development": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "swagger",
      "environmentVariables": {
        "ASPNETCORE_URLS": "https://+:5443;http://+:5000",
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "applicationUrl": "http://localhost:5000"
    },
    "Docker": {
      "commandName": "Docker",
      "launchBrowser": true,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}/swagger",
      "publishAllPorts": true
    }
  }
}